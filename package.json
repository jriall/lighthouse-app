{
  "name": "lighthouse-app",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "coverage:backend": "coverage report -m backend/**/*.py",
    "coverage:frontend": "ng test --watch=false --code-coverage",
    "deploy:staging": "./scripts/deploy.sh staging",
    "e2e": "ng e2e",
    "fix:html": "prettier 'frontend/**/*.html' --write",
    "fix:scss": "stylelint 'frontend/**/*.scss' --fix",
    "fix": "npm run fix:html && npm run fix:html",
    "lint:html": "prettier 'frontend/**/*.html' --check",
    "lint:scss": "stylelint 'frontend/**/*.scss'",
    "lint": "npm run lint:html && npm run lint:html",
    "ng": "ng",
    "server": "python backend/main.py",
    "start": "ng serve",
    "test:backend": "nose2",
    "test:frontend": "ng test",
    "test:frontend:single": "ng test --watch=false --browsers ChromeHeadless"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:frontend:single",
      "pre-push": "npm run test:frontend:single"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/cdk": "^9.2.2",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@ngrx/effects": "^9.1.2",
    "@ngrx/router-store": "^9.1.2",
    "@ngrx/store": "^9.1.2",
    "@ngrx/store-devtools": "^9.1.2",
    "rxjs": "~6.5.4",
    "stackdriver-errors-js": "^0.8.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.51",
    "@types/gapi.client": "^1.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "gts": "^2.0.1",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.0.5",
    "protractor": "~5.4.3",
    "stylelint": "^13.3.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
